<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

	<CheckBoxPreference
		android:key="pref_use_watch_compass"
	    android:defaultValue="true"
	    android:title="@string/pref_title_watch_compass"
	    android:summary="@string/pref_summary_watch_compass"/>

	<!-- For debugging -->
	<Preference
		android:key="button_debug"
		android:title="Debug"
	    android:summary="Select to start service with debug values"/>

</PreferenceScreen>
